{
  "name": "lytovka.com",
  "version": "1.0.0",
  "description": "My personal site",
  "author": "Ivan Lytovka <ivanlytovka@gmail.com> (https://github.com/lytovka)",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lytovka/lytovka.com.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "start": "remix-serve ./build/index.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "lint": "eslint --max-warnings 0 --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "prepare": "husky install",
    "test:coverage": "vitest run --coverage",
    "test": "vitest",
    "typecheck": "tsc --build"
  },
  "dependencies": {
    "@remix-run/dev": "2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "@vercel/remix": "^2.0.1",
    "clsx": "^2.0.0",
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "gray-matter": "^4.0.3",
    "marked": "^5.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@lytovka/eslint-config": "^1.3.0",
    "@tailwindcss/typography": "0.5.10",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@types/eslint": "8.56.0",
    "@types/marked": "5.0.2",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/spotify-api": "^0.0.25",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "^0.34.5",
    "autoprefixer": "10.4.16",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "happy-dom": "12.10.3",
    "husky": "^8.0.2",
    "postcss": "8.4.32",
    "postcss-import": "^15.1.0",
    "prettier": "3.1.1",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.2.2",
    "vitest": "0.34.6"
  }
}
