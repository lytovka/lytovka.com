{
  "name": "lytovka.com",
  "version": "1.0.0",
  "description": "My personal site",
  "author": "Ivan Lytovka <ivanlytovka@gmail.com> (https://github.com/lytovka)",
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lytovka/lytovka.com.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "start": "remix-serve build",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "lint": "eslint --max-warnings 0 --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "postinstall": "remix setup node",
    "prepare": "husky install",
    "test:coverage": "vitest run --coverage",
    "test": "vitest",
    "typecheck": "tsc --build"
  },
  "dependencies": {
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "1.19.3",
    "@vercel/remix": "^1.19.3",
    "clsx": "^2.0.0",
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "gray-matter": "^4.0.3",
    "marked": "^5.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@lytovka/eslint-config": "^1.3.0",
    "@remix-run/dev": "1.19.3",
    "@tailwindcss/typography": "0.5.10",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@types/eslint": "8.44.2",
    "@types/marked": "5.0.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/spotify-api": "^0.0.22",
    "@vitejs/plugin-react": "4.0.4",
    "@vitest/coverage-c8": "0.33.0",
    "autoprefixer": "10.4.15",
    "concurrently": "8.2.1",
    "cross-env": "7.0.3",
    "eslint": "8.49.0",
    "eslint-config-prettier": "8.10.0",
    "happy-dom": "10.11.2",
    "husky": "^8.0.2",
    "postcss": "8.4.25",
    "prettier": "3.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.4"
  }
}
