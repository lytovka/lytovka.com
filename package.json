{
  "name": "remix-app-template",
  "private": true,
  "description": "My personal site",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && cross-env NODE_ENV=production remix build",
    "build:css": "tailwindcss -m -i ./styles/tailwind.css -o ./app/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/tailwind.css -o ./app/styles/tailwind.css",
    "start": "remix-serve api/index.js",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "@remix-run/react": "^1.2.3",
    "@remix-run/vercel": "^1.2.3",
    "date-fns": "^2.28.0",
    "marked": "^4.0.12",
    "parse-md": "^3.0.3",
    "prisma": "^3.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@tailwindcss/typography": "^0.5.2",
    "@types/marked": "^4.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  }
}
