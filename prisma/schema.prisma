generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// TODO: fix model name
model posts {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  date     DateTime @db.Date
  markdown String
  slug     String
  title    String
}
